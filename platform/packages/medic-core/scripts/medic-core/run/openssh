#!/bin/sh

start()
{
  local self="`realpath "$0"`"
  local base="`dirname -- "$self"`"

  source "$base/../env" &&
  merge_environment /srv || return 255

  exec "$PACKAGE_ROOT/sbin/sshd" -D \
    >> "$PACKAGE_STORAGE/openssh/logs/startup.log" 2>&1
}

main()
{
  start
  return "$?"
}

main
exit "$?"

